{
  "expo": {
    "name": "Osonish",
    "slug": "osonish-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "owner": "farakor",
    "extra": {
      "eas": {
        "projectId": "d25e6650-1e06-4ebb-8988-0085861affbf"
      }
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.farakor.osonishmobile",
      "buildNumber": "9",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Это приложение использует доступ к фотогалерее для загрузки изображений к заказам и профилю пользователя.",
        "NSCameraUsageDescription": "Это приложение использует камеру для создания фотографий заказов и профиля пользователя.",
        "NSLocationWhenInUseUsageDescription": "Это приложение использует геолокацию для определения местоположения заказов и поиска ближайших исполнителей.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Это приложение использует геолокацию для определения местоположения заказов и поиска ближайших исполнителей."
      }
    },
    "android": {
      "package": "com.farakor.osonishmobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "navigationBar": {
        "visible": "immersive",
        "backgroundColor": "transparent"
      },
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "com.google.android.c2dm.permission.RECEIVE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": []
        }
      ],
      [
        "expo-location"
      ]
    ]
  }
}