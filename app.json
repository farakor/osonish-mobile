{
  "expo": {
    "name": "Osonish",
    "slug": "osonish-mobile",
    "version": "1.0.3",
    "orientation": "default",
    "sdkVersion": "54.0.0",
    "icon": "./assets/Appicon-appstore.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "owner": "farakor",
    "extra": {
      "eas": {
        "projectId": "d25e6650-1e06-4ebb-8988-0085861affbf"
      }
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.farakor.osonishmobile",
      "buildNumber": "12",
      "icon": "./assets/Appicon-appstore.png",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Это приложение использует доступ к фотогалерее для загрузки изображений к заказам и профилю пользователя.",
        "NSCameraUsageDescription": "Это приложение использует камеру для создания фотографий заказов и профиля пользователя.",
        "NSLocationWhenInUseUsageDescription": "Это приложение использует геолокацию для определения местоположения заказов и поиска ближайших исполнителей.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Это приложение использует геолокацию для определения местоположения заказов и поиска ближайших исполнителей.",
        "LSApplicationQueriesSchemes": [
          "yandexmaps",
          "comgooglemaps"
        ]
      }
    },
    "android": {
      "package": "com.farakor.osonishmobile",
      "versionCode": 4,
      "icon": "./assets/Appicon-playstore.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-foreground.png",
        "backgroundImage": "./assets/adaptive-icon-background.png",
        "backgroundColor": "#679b00",
        "monochromeImage": "./assets/adaptive-icon-monochrome.png"
      },
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "com.google.android.c2dm.permission.RECEIVE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.SCHEDULE_EXACT_ALARM"
      ],
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan",
      "statusBarStyle": "auto",
      "navigationBarStyle": "auto",
      "enableEdgeToEdge": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "yandexmaps"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": []
        }
      ],
      [
        "expo-location"
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Это приложение использует доступ к фотогалерее для загрузки изображений к заказам и профилю пользователя.",
          "cameraPermission": "Это приложение использует камеру для создания фотографий заказов и профиля пользователя."
        }
      ]
    ]
  }
}